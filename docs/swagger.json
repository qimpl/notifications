{
    "swagger": "2.0",
    "info": {
        "title": "Notifications API",
        "contact": {},
        "license": {},
        "version": "0.1.0"
    },
    "basePath": "/api/v1",
    "paths": {
        "/notification": {
            "post": {
                "description": "Add a new notification into the broker queues",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Notifications"
                ],
                "summary": "Add notification",
                "parameters": [
                    {
                        "description": "Notification object",
                        "name": "Notification",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/models.Notification"
                        }
                    }
                ],
                "responses": {
                    "204": {},
                    "422": {
                        "description": "Unprocessable Entity",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "models.Notification": {
            "type": "object",
            "properties": {
                "email": {
                    "type": "string",
                    "example": "john.doe@foo.com"
                },
                "expo_push_token": {
                    "type": "string",
                    "example": "ExponentPushToken[xxxxxxxxxxxxxxxxxxxxxx]"
                },
                "message": {
                    "type": "string",
                    "example": "Hello my friend"
                },
                "title": {
                    "type": "string",
                    "example": "My Notification"
                },
                "type": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    },
                    "example": [
                        "[\"email\"",
                        " \"push\"]"
                    ]
                }
            }
        }
    }
}